<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://xmlns.jcp.org/xml/ns/javaee"
		version="3.0"
		xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" >
		<display-name>Cliente</display-name>
		<servlet>
			<servlet-name>PaginaCliente</servlet-name>
			<servlet-class>Gestione.Allenamenti.PaginaCliente</servlet-class>
		</servlet>

		<servlet-mapping>
			<servlet-name>PaginaCliente</servlet-name>
			<url-pattern>/PaginaCliente</url-pattern>
		</servlet-mapping>
		
		<display-name>Registrzione</display-name>
		<servlet>
			<servlet-name>Registrazione</servlet-name>
			<servlet-class>Gestione.Allenamenti.Registrazione</servlet-class>
		</servlet>

		<servlet-mapping>
			<servlet-name>Registrazione</servlet-name>
			<url-pattern>/Registrazione</url-pattern>
		</servlet-mapping>
		
		<display-name>SceltaTrainer</display-name>
		<servlet>
			<servlet-name>SceltaTrainer</servlet-name>
			<servlet-class>Gestione.Allenamenti.SceltaTrainer</servlet-class>
		</servlet>

		<servlet-mapping>
			<servlet-name>SceltaTrainer</servlet-name>
			<url-pattern>/SceltaTrainer</url-pattern>
		</servlet-mapping>	
	</web-app>